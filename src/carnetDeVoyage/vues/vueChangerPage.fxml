<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox alignment="CENTER" prefHeight="104.0" prefWidth="950.0" spacing="77.0" styleClass="glass-grey" stylesheets="@../ressources/style/buttonStyle.css" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1" fx:controller="carnetDeVoyage.vues.VueChangerPage">
   <children>
      <Button fx:id="supprimerPageGauche" mnemonicParsing="false" onAction="#supprimerPageGauche" prefHeight="58.0" prefWidth="42.0" styleClass="bevel-grey">
         <font>
            <Font size="26.0" />
         </font>
         <graphic>
            <ImageView fitHeight="53.0" fitWidth="52.0" pickOnBounds="true" preserveRatio="true" styleClass="bevel-grey">
               <image>
                  <Image url="@../ressources/image/supprimerPage.png" />
               </image>
            </ImageView>
         </graphic>
         <tooltip>
            <Tooltip fx:id="tooltip1" text="Supprimer la page à gauche" />
         </tooltip>
      </Button>
      <HBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="77.0" prefWidth="505.0" spacing="69.0">
         <children>
                  <Button fx:id="back" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#retourALaPagePrecedante" prefHeight="61.0" prefWidth="69.0" styleClass="bevel-grey" text="&lt;">
                     <font>
                        <Font size="29.0" />
                     </font>
               <tooltip>
                  <Tooltip fx:id="tooltip3" text="Passer à la Page Précédente" />
               </tooltip>
                  </Button>
                        <TextField fx:id="pageActuel" alignment="CENTER" prefHeight="49.0" prefWidth="85.0">
                           <font>
                              <Font size="26.0" />
                           </font>
                        </TextField>
                  <Button fx:id="next" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#passerALaPageSuivante" prefHeight="62.0" prefWidth="69.0" styleClass="bevel-grey" text="&gt;">
                     <font>
                        <Font size="29.0" />
                     </font>
               <tooltip>
                  <Tooltip fx:id="tooltip4" text="Passer à la Page Suivante" />
               </tooltip>
                  </Button>
         </children>
      </HBox>
      <Button fx:id="supprimerPageDroite" mnemonicParsing="false" onAction="#supprimerPageDroite" prefHeight="34.0" prefWidth="92.0" styleClass="bevel-grey" stylesheets="@../ressources/style/buttonStyle.css">
         <font>
            <Font size="27.0" />
         </font>
         <tooltip>
            <Tooltip fx:id="tooltip2" text="Supprimer la page à droite" />
         </tooltip>
         <graphic>
            <ImageView fitHeight="53.0" fitWidth="52.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../ressources/image/supprimerPage.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
   </children>
</HBox>
